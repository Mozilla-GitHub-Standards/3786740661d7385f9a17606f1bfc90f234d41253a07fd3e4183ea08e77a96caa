<?xml version='1.0' encoding='UTF-8'?>
<project>
  <actions/>
  <description></description>
  <logRotator class="hudson.tasks.LogRotator">
    <daysToKeep>10</daysToKeep>
    <numToKeep>30</numToKeep>
    <artifactDaysToKeep>-1</artifactDaysToKeep>
    <artifactNumToKeep>-1</artifactNumToKeep>
  </logRotator>
  <keepDependencies>false</keepDependencies>
  <properties/>
  <scm class="org.jenkinsci.plugins.multiplescms.MultiSCM" plugin="multiple-scms@0.4">
    <scms>
      <hudson.plugins.git.GitSCM plugin="git@2.3.5">
        <configVersion>2</configVersion>
        <userRemoteConfigs>
          <hudson.plugins.git.UserRemoteConfig>
            <url>https://github.com/nils-ohlmeier/mozqawebrtc-net-overlay.git</url>
          </hudson.plugins.git.UserRemoteConfig>
        </userRemoteConfigs>
        <branches>
          <hudson.plugins.git.BranchSpec>
            <name>*/master</name>
          </hudson.plugins.git.BranchSpec>
        </branches>
        <doGenerateSubmoduleConfigurations>false</doGenerateSubmoduleConfigurations>
        <submoduleCfg class="list"/>
        <extensions/>
      </hudson.plugins.git.GitSCM>
    </scms>
  </scm>
  <assignedNode>network-steeplechase</assignedNode>
  <canRoam>false</canRoam>
  <disabled>true</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers>
    <org.jenkinsci.plugins.urltrigger.URLTrigger plugin="urltrigger@0.37">
      <spec></spec>
      <entries>
        <org.jenkinsci.plugins.urltrigger.URLTriggerEntry>
          <url>http://192.168.0.7/view/tests/job/tests-beta-linux64/ws/releases/firefox-latest-beta.en-US.linux-x86_64.tests.zip</url>
          <proxyActivated>false</proxyActivated>
          <checkStatus>false</checkStatus>
          <statusCode>200</statusCode>
          <timeout>300</timeout>
          <checkETag>false</checkETag>
          <checkLastModificationDate>true</checkLastModificationDate>
          <inspectingContent>false</inspectingContent>
          <contentTypes/>
        </org.jenkinsci.plugins.urltrigger.URLTriggerEntry>
      </entries>
      <labelRestriction>false</labelRestriction>
    </org.jenkinsci.plugins.urltrigger.URLTrigger>
  </triggers>
  <concurrentBuild>false</concurrentBuild>
  <builders>
    <hudson.tasks.Shell>
      <command>export DEST=/home/mozilla/firefoxes/beta/linux64
mkdir -p $DEST
cd $DEST
rm -rf tests
mkdir -p tests
cd tests

curl http://192.168.0.7/view/tests/job/tests-beta-linux64/ws/releases/firefox-latest-beta.en-US.linux-x86_64.tests.zip &gt; firefox-latest-beta.en-US.linux-x86_64.tests.zip

unzip -u -o firefox-latest-beta.en-US.linux-x86_64.tests.zip &apos;steeplechase/*&apos; &apos;mochitest/tests/dom/media/tests/*&apos;

cp $WORKSPACE/turnConfig.js $DEST/tests/steeplechase/tests/dom/media/tests/mochitest/
# cp $WORKSPACE/aurora/pc.js $DEST/tests/steeplechase/tests/dom/media/tests/mochitest/
# cp $WORKSPACE/templates.js $DEST/tests/steeplechase/tests/dom/media/tests/mochitest/

# Note: this leads to intermitten failures.
# cp $WORKSPACE/steeplechase.ini $DEST/tests/steeplechase/tests/dom/media/tests/mochitest/
cp $DEST/tests/mochitest/tests/dom/media/tests/mochitest/test_peerConnection_basicVideo.html $DEST/tests/steeplechase/tests/dom/media/tests/mochitest/
cp $DEST/tests/mochitest/tests/dom/media/tests/mochitest/test_peerConnection_basicAudioVideoCombined.html $DEST/tests/steeplechase/tests/dom/media/tests/mochitest/</command>
    </hudson.tasks.Shell>
